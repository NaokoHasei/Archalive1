                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          CASE WHEN S2.PCNAME IS NULL 
                      THEN S1.SYSTEMFILE ELSE isnull(S2.SYSTEMFILE, '') END AS SYSTEMFILE, 
                      CASE WHEN S2.PCNAME IS NULL 
                      THEN S1.SECTIONNAME ELSE isnull(S2.SECTIONNAME, '') 
